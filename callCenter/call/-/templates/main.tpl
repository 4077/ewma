<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    {SHOW_OUTPUT_BUTTON}

    <div class="call_button {COLOR_CLASS}" hover="hover" hover_delay="1000">
        <div class="buttons" hover="hover">
            {SETTINGS_BUTTON}
            {RENAME_BUTTON}
            {DUPLICATE_BUTTON}
            {DELETE_BUTTON}
        </div>
        <div class="content">
            {CONTENT}
        </div>
        <div class="cb"></div>
    </div>
    <div class="cb"></div>

    <!-- settings -->
    <div class="settings">
        <div class="path">
            {PATH}
        </div>
        <div class="data">
            {DATA}
        </div>
        <div class="inputs">
            {INPUTS}
        </div>
        <div class="buttons">
            {REQUIRE_CONFIRMATION_TOGGLE_BUTTON}
        </div>
        <div class="cb"></div>
    </div>
    <!-- / -->

</div>
